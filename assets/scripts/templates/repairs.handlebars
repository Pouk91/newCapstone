{{#each repairs as |repair|}}

  {{> partial}}
  <!-- Don't uncomment yet -->

  <!-- Developers, why are we using data-id and not just i d?-->
  <dl class='repairs-list' data-id="{{repair.id}}">
    <dt>Id:</dt>
    <dd>{{repair.id}}</dd>
    <dt>Year:</dt>
    <dd>{{repair.year}}</dd>
    <dt>Make:</dt>
    <dd>{{repair.make}}</dd>
    <dt>Model:</dt>
    <dd>{{repair.model}}</dd>
    <dt>Repair Name:</dt>
    <dd>{{repair.repairname}}</dd>
    <dt>Repair Cost:</dt>
    <dd>{{repair.repaircost}}</dd </dl>
{{/each}}
